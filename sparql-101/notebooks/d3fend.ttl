    d3f:may-access d3f:DatabaseFile ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty d3f:may-access ;
            owl:someValuesFrom d3f:DatabaseFile ],
        [ a owl:Restriction ;
