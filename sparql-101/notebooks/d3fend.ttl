    rdfs:subClassOf d3f:UnsupervisedPreprocessing .

d3f:Semi-supervisedGenerativeModelLearning a owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Semi-supervised Generative Model Learning" ;
