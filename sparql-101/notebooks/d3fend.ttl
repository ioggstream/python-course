    d3f:kb-mitre-analysis "" ;
    d3f:kb-organization "MITRE" ;
    d3f:kb-reference-of d3f:ServiceBinaryVerification ;
    d3f:kb-reference-title "CAR-2014-02-001: Service Binary Modifications" .

